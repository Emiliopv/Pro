      "source": [
        "def quickSort(L,res):\n",
        "  piv = L.pop()\n",
        "  if len(L) == 0:\n",
        "    res.append(piv)\n",
        "    return None\n",
        "  mayor = []\n",
        "  menor = []\n",
        "  for e in L:\n",
        "    mayor.append(e) if piv >= e else menor.append(e)\n",
        "  if len(mayor) >= 1:\n",
        "    __quickSort__(mayor,res)\n",
        "  res.append(piv)\n",
        "  if len(menor) >= 1:\n",
        "    __quickSort__(menor,res)\n",
        "\n",
        "def quickSort(L):\n",
        "  res = []\n",
        "  __quickSort__(L,res)\n",
        "  return res"
      ]
    },
